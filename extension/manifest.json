{
  "manifest_version": 3,
  "name": "secutI",
  "version": "1.0",
  "description": "Anti-Phishing & Secure Download Manager",
  "permissions": [
    "scripting",
    "activeTab",
    "storage",
    "declarativeNetRequest",
    "downloads",
    "notifications"
  ],
"host_permissions": [
  "https://archive.org/*",
  "<all_urls>"
],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "config.js",
        "utils.js",
      
        
        
        "ui.js",
        "impersonation.js",
        "detector.js",
        "content_guard.js",
        "main.js"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "ui/aurora.css",
        "ui/overlay.html",
        "plugins/plugins.json"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}